.root {
    flex: 0 0;
    height: fit-content;
    min-height: 100vh;
    min-width: 15%;
    position: relative;
    z-index: 9999;

    .title {
        font-weight: 500;
        margin: 1rem 0 0 0;
    }

    .paper {
        height: 100%;
        position: absolute;
        width: 100%;
    }

    .link {
        color: inherit;
        text-decoration: none;
    }
}
